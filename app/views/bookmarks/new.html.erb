 <div class="container">
    <%= simple_form_for [ @list, @bookmark ] do |f| %>
      <div class="form-group mb-3">
        <%= f.input :comment, class: "form-control" %>
      </div>
      <div class="form-group mb-3">
        <%= f.association :movie, class: "form-control" %>
      </div>
      <%= f.submit "Submit bookmark", class: "btn btn-primary mb-3" %>
    <% end %>
</div>
